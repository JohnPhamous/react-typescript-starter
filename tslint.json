{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "defaultSeverity": "error",
  "rules": {
    "jsx-wrap-multiline": false
  },
  "jsRules": {},
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/serviceWorker.ts"
    ]
  }
}
